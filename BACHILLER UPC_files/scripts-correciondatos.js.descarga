function refrescarC(){
	var urlBase = '/';
	captcha = $("#captchaImg, #captchaImgPriv");
	var img = $('<img>');
	var d = new Date();
	var n = d.getMilliseconds();
	img.attr('src',urlBase + 'simplecaptcha?date=' + n);
	captcha.empty();
	captcha.append(img);
	$('#captcha, #captchaPriv').val('').focus();
}

